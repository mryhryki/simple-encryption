{
  "name": "@mryhryki/simple-encryption",
  "version": "0.1.0",
  "description": "Simple encryption/decryption library for Node.js/Deno/Browser.",
  "main": "index.js",
  "engines": {
    "node": ">=16",
    "deno": ">=1.17",
    "bun": ">=0.6"
  },
  "scripts": {
    "test": "deno test ./src/",
    "test:node": "node -r esbuild-register ./src/test/index_node.ts",
    "test:deno": "deno run ./src/test/index.ts",
    "test:bun": "bun run ./src/test/index.ts",
    "fmt": "deno fmt ./src/",
    "check": "deno check ./src/index.ts"
  },
  "keywords": [
    "simple",
    "encryption",
    "decryption",
    "crypto"
  ],
  "author": "mryhryki",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.18.15",
    "esbuild-register": "^3.4.2"
  }
}
